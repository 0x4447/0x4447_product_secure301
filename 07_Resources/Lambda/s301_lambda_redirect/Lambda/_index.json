{
	"LambdaRedirect": {
		"Type": "AWS::Lambda::Function",
		"Properties": {
			"FunctionName": { "Fn::Sub": "redirect_${UniqueParam}" },
			"Code": {
				"ZipFile": "exports.handler = async (event) => {return true;};"
			},
			"Handler": "index.handler",
			"MemorySize": 128,
			"Role": { "Fn::GetAtt": ["LambdaRedirectRole", "Arn"] },
			"Runtime": "nodejs12.x",
			"Timeout": 30,
			"Environment": {
				"Variables": {
					"URL": { "Ref": "UrlToParam" }
				}
			}
		}
	}
}
